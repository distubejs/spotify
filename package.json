{
  "name": "@distube/spotify",
  "version": "1.4.2",
  "description": "A DisTube custom plugin for supporting Spotify.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,ts,json,yml,yaml,md}\"",
    "build": "tsup",
    "build:check": "tsc --noEmit",
    "postinstall": "husky install",
    "prepack": "yarn build && pinst -d",
    "postpack": "pinst -e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/spotify.git"
  },
  "keywords": [
    "distube",
    "plugin",
    "spotify",
    "discord",
    "music"
  ],
  "author": "Skick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distubejs/spotify/issues"
  },
  "homepage": "https://github.com/distubejs/spotify#readme",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@types/jest": "^29.0.0",
    "@types/spotify-web-api-node": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "^29.0.1",
    "commitlint": "^17.1.2",
    "discord.js": "^14.3.0",
    "distube": "^4.0.3",
    "eslint": "^8.23.0",
    "eslint-config-distube": "^1.6.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-jsdoc": "^39.3.6",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "nano-staged": "^0.8.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0",
    "spotify-uri": "^3.0.3",
    "spotify-url-info": "^3.1.4",
    "spotify-web-api-node": "^5.0.2"
  },
  "peerDependencies": {
    "distube": "^3.3.1||4"
  },
  "nano-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
