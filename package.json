      "eslint"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "distube": "^3.3.1||4"
  },
  "scripts": {
    "jest": "jest",
    "test": "bun --env-file=.env.test run jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,ts,json,yml,yaml,md}\"",
    "build": "tsup",
