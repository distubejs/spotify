    "commitlint": "^19.2.2",
    "discord.js": "^14.14.1",
    "distube": "^4.2.0",
    "eslint": "^8.57.0",
    "eslint-config-distube": "^1.7.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nano-staged": "^0.8.0",
    "pinst": "^3.0.0",
