{
  "name": "@distube/spotify",
  "version": "1.1.0",
  "description": "A DisTube custom plugin for supporting Spotify.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write **/*.{js,ts,json,yml,yaml,md}",
    "build": "tsc",
    "build:check": "tsc --noEmit --incremental false",
    "npm:publish": "npm publish --access public",
    "postinstall": "husky install",
    "prepublishOnly": "pinst -d",
    "postpublish": "pinst -e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/spotify.git"
  },
  "keywords": [
    "distube",
    "plugin",
    "spotify",
    "discord",
    "music"
  ],
  "author": "Skick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distubejs/spotify/issues"
  },
  "homepage": "https://github.com/distubejs/spotify#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@discordjs/opus": "^0.7.0",
    "@types/spotify-web-api-node": "^5.0.6",
    "commitlint": "^16.0.2",
    "discord.js": "^13.5.1",
    "distube": "^3.1.0",
    "eslint": "^8.6.0",
    "eslint-config-distube": "^1.6.3",
    "husky": "^7.0.4",
    "libsodium-wrappers": "^0.7.9",
    "lint-staged": "^12.1.7",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "spotify-uri": "^2.2.0",
    "spotify-url-info": "^2.2.3",
    "spotify-web-api-node": "^5.0.2"
  },
  "peerDependencies": {
    "distube": "^3.1.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
